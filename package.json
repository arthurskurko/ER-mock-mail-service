{
  "name": "mock-graph-mail",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev --host 0.0.0.0 --port 5173",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 5173",
    "cf:dev": "npm run db:migrate:local && npm run build && wrangler pages dev .svelte-kit/cloudflare --ip 0.0.0.0 --port 5173 --local",
    "cf:deploy": "npm run build && wrangler deploy",
    "db:migrate:local": "wrangler d1 execute mock_graph_mail --local --file migrations/0001_init.sql && wrangler d1 execute mock_graph_mail --local --file migrations/0002_add_to_name.sql",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^4.9.0",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/node": "^22.10.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.3.0",
    "vitest": "^1.6.0",
    "wrangler": "^3.114.17",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
}